<div class="userList">
	{{# Users }}
	<div>
		{{# Avatar }}
			<img alt="{{ Name }}'s Steam Avatar"
				src="https://fpp.literallybrian.com/steamcommunity/public/images/avatars/{{ AvatarFolder }}/{{ Avatar }}_full.jpg">
		{{/ Avatar }}
		{{^ Avatar }}
			<img alt="RohBot User" src="rohbot.png">
		{{/ Avatar }}
		<div data-color="{{ Color }}" data-rank="{{ Rank }}">
			{{# Web }}
				{{ Name }}
			{{/ Web }}
			{{^ Web }}
				<a href="https://steamcommunity.com/profiles/{{ UserID }}" target="_blank">
					{{ Name }}
				</a>
			{{/ Web }}
			{{# Playing }}
				<br>
				In-Game:&nbsp;
				{{ Playing }}&nbsp;
			{{/ Playing }}
		</div>
	</div>
	{{/ Users }}
</div>
